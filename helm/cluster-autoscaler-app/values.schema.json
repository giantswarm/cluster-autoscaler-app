{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "azure": {
            "type": "object",
            "properties": {
                "managedIdentity": {
                    "type": "boolean"
                },
                "subscriptionID": {
                    "type": "string"
                },
                "vmType": {
                    "type": "string"
                }
            }
        },
        "clusterID": {
            "type": "string"
        },
        "configmap": {
            "type": "object",
            "properties": {
                "balanceSimilarNodeGroups": {
                    "type": "string"
                },
                "balancingIgnoreLabels": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "expander": {
                    "type": "string"
                },
                "newPodScaleUpDelay": {
                    "type": "string"
                },
                "scaleDownUnneededTime": {
                    "type": "string"
                },
                "scaleDownUtilizationThreshold": {
                    "type": "number"
                },
                "scanInterval": {
                    "type": "string"
                },
                "skipNodesWithLocalStorage": {
                    "type": "string"
                },
                "skipNodesWithSystemPods": {
                    "type": "string"
                }
            }
        },
        "global": {
            "type": "object",
            "properties": {
                "podSecurityStandards": {
                    "type": "object",
                    "properties": {
                        "enforced": {
                            "type": "boolean"
                        }
                    }
                }
            }
        },
        "groupID": {
            "type": "integer"
        },
        "image": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "registry": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "isManagementCluster": {
            "type": "boolean"
        },
        "managementCluster": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                }
            }
        },
        "name": {
            "type": "string"
        },
        "namespace": {
            "type": "string"
        },
        "node": {
            "type": "object",
            "properties": {
                "caBundlePath": {
                    "type": "string"
                },
                "nodeSelector": {
                    "type": "object",
                    "properties": {
                        "node-role.kubernetes.io/control-plane": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "provider": {
            "type": "string"
        },
        "registry": {
            "type": "object",
            "properties": {
                "domain": {
                    "type": "string"
                }
            }
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "userID": {
            "type": "integer"
        },
        "verticalPodAutoscaler": {
            "type": "object",
            "properties": {
                "minAllowed": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    }
}
